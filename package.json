{
  "name": "forge-node-cli",
  "version": "1.1.0",
  "description": "Forge a nodejs express project with server and database service layer",
  "keywords": [
    "nodejs",
    "express",
    "knex",
    "scaffold",
    "api"
  ],
  "homepage": "https://github.com/kumar8317/forge-node",
  "bugs": "https://github.com/kumar8317/forge-node/issues",
  "repository": "kumar8317/forge-node",
  "license": "MIT",
  "author": "Ankit Kumar",
  "type": "module",
  "exports": "./lib/index.js",
  "main": "",
  "types": "dist/index.d.ts",
  "bin": {
    "forge-node-cli": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "forge",
    "commands": "./dist/commands",
    "dirname": "example",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^4",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.4.19",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}